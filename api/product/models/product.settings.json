{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Product name #",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "qty": {
      "type": "integer"
    },
    "categories": {
      "via": "products",
      "collection": "category"
    }
  }
}
